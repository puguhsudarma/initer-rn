{
  "name": "autoappcustomer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "jetify": "./node_modules/.bin/jetify",
    "clear-android": "cd android/ && ./gradlew clean && ./gradlew cleanBuildCache && cd ..",
    "run-android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug && cd ..",
    "release-android:apk": "yarn jetify && cd ./android && ./gradlew app:assembleRelease && cd ..",
    "release-android:bundle": "yarn jetify && cd ./android && ./gradlew app:bundleRelease && cd ..",
    "pretty": "prettier --semi true --single-quote --trailing-comma all --tab-width 4 --write 'src/**/*.{ts,tsx}'",
    "watchman-fix": "echo fs.inotify.max_user_watches=524288 | sudo tee -a /etc/sysctl.conf && sudo sysctl -p",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn pretty",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.8.1",
    "axios": "^0.21.1",
    "date-fns": "^2.10.0",
    "lodash": "^4.17.15",
    "react": "16.9.0",
    "react-dom": "^16.13.0",
    "react-native": "0.61.5",
    "react-native-animatable": "^1.3.3",
    "react-native-extra-dimensions-android": "^1.2.5",
    "react-native-modal": "^11.5.4",
    "react-native-navigation": "^6.1.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-logic": "^2.1.1",
    "redux-persist": "^6.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.23",
    "@types/react-native": "^0.61.20",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-logger": "^3.0.7",
    "babel-jest": "^25.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.9.0",
    "redux-logic-test": "^2.0.0",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.js"
    ],
    "collectCoverage": true
  }
}
